{
  "type": "module",
  "name": "videomark",
  "version": "2.5.1",
  "repository": "git@github.com:videomark/videomark.git",
  "license": "Apache-2.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "bin/build",
    "build:watch": "MODE=production vite build --watch --outDir dist/production",
    "build-safari": "bin/build && bin/build-safari",
    "build-android": "bin/build && bin/build-android",
    "test": "pnpm e2e",
    "e2e": "run-s build e2e:*",
    "e2e:extension": "pnpm --filter @videomark/e2e-test test",
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "npm-run-all lint:*",
    "lint:cspell": "cspell --no-progress",
    "lint:svelte": "svelte-check --compiler-warnings 'missing-custom-element-compile-options:ignore'",
    "lint:prettier": "prettier --check --plugin prettier-plugin-svelte .",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "lint-report": "eslint --output-file eslint_report.json --format json --ext .js,.jsx .",
    "version": "bin/version"
  },
  "devDependencies": {
    "@playwright/test": "^1.37.1",
    "@sveltejs/vite-plugin-svelte": "^2.4.5",
    "cspell": "^7.0.0",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-svelte": "^2.32.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.2",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-scss": "^5.1.0",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "svelte-i18n": "^3.7.0",
    "svelte-preprocess": "^5.0.4",
    "vite": "^4.4.9"
  },
  "dependencies": {
    "@sveltia/ui": "^0.6.0",
    "@videomark/sparkline": "github:videomark/sparkline",
    "mpd-parser": "^1.2.1",
    "msgpack-lite": "^0.1.26",
    "uuid": "^9.0.0"
  }
}

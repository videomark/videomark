{
  "type": "module",
  "name": "videomark",
  "version": "3.0.0",
  "repository": "git@github.com:videomark/videomark.git",
  "license": "Apache-2.0",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "build": "bin/build",
    "build:watch": "MODE=production vite build --watch --outDir dist/production",
    "build-safari": "bin/build && bin/build-safari",
    "build-android": "bin/build && bin/build-android",
    "test": "npm-run-all test:*",
    "test:e2e": "playwright test",
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "lint": "npm-run-all lint:*",
    "lint:cspell": "cspell --no-progress",
    "lint:svelte": "svelte-check --compiler-warnings 'missing-custom-element-compile-options:ignore'",
    "lint:prettier": "prettier --check --plugin prettier-plugin-svelte .",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint '**/*.{css,scss,svelte}'",
    "lint-report": "eslint --output-file eslint_report.json --format json --ext .js,.jsx .",
    "version": "bin/version"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "cspell": "^8.1.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-svelte": "^2.35.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.69.5",
    "stylelint": "^16.0.2",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-scss": "^6.0.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-i18n": "^4.0.0",
    "svelte-preprocess": "^5.1.2",
    "vite": "^5.0.10"
  },
  "dependencies": {
    "@sveltia/ui": "^0.9.0",
    "@videomark/sparkline": "github:videomark/sparkline",
    "mpd-parser": "^1.3.0",
    "msgpack-lite": "^0.1.26",
    "uuid": "^9.0.1"
  }
}

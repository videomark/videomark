#!/bin/bash
set -euo pipefail

PUBLISH_DIR=dist
NODE_MODULES=node_modules
TEST_E2E=test/e2e

log() {
  echo "$0:" "$@"
}

cd -- "$(dirname -- "$0")/.."

log "Working directory is: $(pwd)"

log cleanup
rm -rf -- ${PUBLISH_DIR}
rm -rf -- ${NODE_MODULES}

log ${TEST_E2E}
rm -rf -- ${TEST_E2E}/${NODE_MODULES}

#!/bin/bash
# package.json のバージョンアップ。拡張機能に含まれる manifest.json の version はビルド時に package.json から
# コピーされるので、ここでは変更不要。
set -euo pipefail
cd -- "$(dirname -- "$0")/.."
export VIDEOMARK_VERSION=${VIDEOMARK_VERSION:-$(node -pe 'require("./package.json").version')}
version() {
  echo "${VIDEOMARK_VERSION}"
}
version
git add .

{
  "name": "@videomark/videomark-log-view",
  "version": "1.12.1",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.11.4",
    "@mui/material": "^5.0.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/styles": "^5.0.0",
    "@nivo/bar": "^0.81.0",
    "@nivo/calendar": "^0.81.0",
    "@nivo/core": "^0.81.0",
    "@nivo/scatterplot": "0.81.0",
    "@svgr/webpack": "^7.0.0",
    "@videomark/videomark-mini-stats": "file:../videomark-mini-stats",
    "@babel/eslint-parser": "^7.11.0",
    "babel-jest": "^29.0.0",
    "babel-loader": "^9.0.0",
    "babel-preset-react-app": "^10.0.0",
    "bfj": "^7.0.2",
    "chalk": "^5.0.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "css-loader": "^6.0.0",
    "dataframe-js": "^1.4.3",
    "date-fns": "^2.15.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.0.0",
    "fs-extra": "^11.0.0",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^29.0.0",
    "jest-pnp-resolver": "^1.2.2",
    "jest-resolve": "^29.0.0",
    "material-table": "^2.0.0",
    "material-ui-flat-pagination": "^4.1.1",
    "mini-css-extract-plugin": "^2.0.0",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "p-iteration": "^1.1.8",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^8.0.0",
    "prettier": "^2.0.5",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^18.0.0",
    "react-router": "^6.0.0",
    "react-router-dom": "^6.0.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.0.0",
    "url-loader": "^4.1.0",
    "uuid": "^9.0.0",
    "web-streams-polyfill": "^3.0.0",
    "webpack": "^5.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-subresource-integrity": "^5.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "pnpm build-extension-prod && pnpm build-extension-dev",
    "build-extension-prod": "cross-env PUBLIC_URL=/qoelog BASE_URL=https://sodium.webdino.org node scripts/build.js && rm -rf build-prod && cp -r build build-prod && rm -r build",
    "build-extension-dev": "cross-env PUBLIC_URL=/qoelog BASE_URL=https://localhost node scripts/build.js && rm -rf build-dev && cp -r build build-dev && rm -r build",
    "build-android": "cross-env BASE_URL=https://sodium.webdino.org node scripts/build.js && rimraf build/favicon.png build/unsupported.html build/static/js/*.LICENSE*",
    "build-web": "cross-env BASE_URL=https://sodium.webdino.org node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "eslint --ext .js,.jsx src",
    "lint-report": "eslint --output-file eslint_report.json --format json --ext .js,.jsx src",
    "format": "prettier --write src config scripts"
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "//": "pnpm との相性問題を回避 https://jestjs.io/docs/configuration#transformignorepatterns-arraystring",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
